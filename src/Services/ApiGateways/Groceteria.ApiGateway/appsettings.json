{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "LoggerConfigOptions": {
    "OverrideSource": "Microsoft",
    "OutputTemplate": "{NewLine} [{Timestamp:yyyy-MM-dd HH:mm:ss} {Level:u3}] {Properties} {Message} {Exception}"
  },
  "Elasticsearch": {
    "Uri": "http://localhost:9200"
  }
}
