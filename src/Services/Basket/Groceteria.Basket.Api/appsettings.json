{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "LoggerConfigOption": {
    "OverrideSource": "Microsoft",
    "OutputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss} {Level:u3}] {NewLine} {Properties} {NewLine} {NewLine} {Message} {Exception}"
  },
  "Elasticsearch": {
    "Uri": "http://localhost:9200"
  },
  "RedisCache": {
    "InstanceName": "Groceteria.Basket.Cache",
    "ConnectionString": "localhost:6379"
  },
  "CatalogueApiSettings": {
    "baseAddress": "http://localhost:5000",
    "version": "v2"
  },
  "GrpcSettings": {
    "DiscountUrl": "http://localhost:5002"
  },
  "RabbitMq": {
    "ConnectionString": "amqp://user:Passw0rd@localhost:5672"
  }
}
