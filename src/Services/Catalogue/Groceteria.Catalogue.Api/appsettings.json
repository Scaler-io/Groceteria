{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "LoggerConfigOption": {
    "OverrideSource": "Microsoft",
    "OutputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss} {Level:u3}] {NewLine} {Properties} {NewLine} {NewLine} {Message} {Exception}"
  },
  "Elasticsearch": {
    "Uri": "http://localhost:9200"
  },
  "MongodbSettings": {
    "ConnectionString": "mongodb://localhost:27017",
    "Database": "catalogueDb"
  }
}
